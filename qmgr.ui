<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="tk.Toplevel" id="window">
    <property name="geometry">600x800</property>
    <property name="height">800</property>
    <property name="maxsize">600|800</property>
    <property name="minsize">600|800</property>
    <property name="title" translatable="yes">qmgr - qemu manager</property>
    <property name="width">600</property>
    <child>
      <object class="ttk.Treeview" id="vmTree">
        <property name="selectmode">extended</property>
        <layout manager="pack">
          <property name="anchor">center</property>
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">bottom</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="infoFrame">
        <property name="height">50</property>
        <property name="relief">raised</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="anchor">n</property>
          <property name="expand">false</property>
          <property name="fill">x</property>
          <property name="side">top</property>
        </layout>
        <containerlayout manager="pack">
          <property name="propagate">False</property>
        </containerlayout>
        <child>
          <object class="ttk.Label" id="vmName">
            <property name="anchor">w</property>
            <property name="font">{arial} 16 {bold}</property>
            <property name="justify">center</property>
            <property name="state">normal</property>
            <property name="text" translatable="yes">qmgr</property>
            <property name="width">25</property>
            <layout manager="pack">
              <property name="anchor">n</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="startButton">
            <property name="text" translatable="yes">Start</property>
            <layout manager="pack">
              <property name="padx">10</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="modifyButton">
            <property name="text" translatable="yes">Modify</property>
            <layout manager="pack">
              <property name="padx">10</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="deleteButton">
            <property name="text" translatable="yes">Delete</property>
            <layout manager="pack">
              <property name="padx">10</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
